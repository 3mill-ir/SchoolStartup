@model SchoolService.Models.DataModel.AdminNomreReport_Model
<script>
    jQuery(document).ready(function () {
        TableData.init();
    });
</script>
<div class="col-lg-6 col-md-6">
    <table class="table table-striped table-bordered table-hover table-full-width dataTable no-footer" id="sample_1">
        <thead>
            <tr>
                <th class="center">تاریخ</th>
                <th class="center">نمره کلاسی </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.NomarateKelasi)
            {
                <tr>
                    <td class="center">@SchoolService.Areas.Admin3mill.Models.Tools.JalaliDateWithoutHour(item.Tarikh)</td>
                    <td class="center">@item.NomreDars</td>
                </tr>
            }
        </tbody>
    </table>
</div>
<div class="col-lg-4 col-md-4">
    <table class="table table-striped table-bordered table-hover table-full-width dataTable no-footer" id="sample_2">
        <thead>
            <tr>
                <th class="center">تاریخ</th>
                <th class="center">نمره امتحانی</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.NomarateEmtehani)
            {
                <tr>
                    <td class="center">@SchoolService.Areas.Admin3mill.Models.Tools.JalaliDateWithoutHour(item.Tarikh)</td>
                    <td class="center">@item.NomreDars</td>
                </tr>
            }
        </tbody>
    </table>
</div>